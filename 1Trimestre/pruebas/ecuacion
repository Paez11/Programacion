{
    float (a,b,c,r1,r2);
    z=b^2-4ac;
    System.out.printIn("Introduzca las variables");
    read(a);
    read(b);
    read(c);
    if (a==0){
        print ("no hay solucion");
    }else
        r1=(-b+(math.sqrt(z))/2a);
        r2=(-b-(math.sqrt(z))/2a);
    }else if (z==-z){
        System.out.printIn("no hay solucion");
    }
    System.out.printIn("la solucion es"+r1,r2);
}